{
    "manifest_version": 3,
    "name": "Orreilly Downloader",
    "version": "0.0.1",
    "permissions": [
      "activeTab",
      "alarms",
      "tabs",
      "webNavigation",
      "notifications",
      "clipboardRead",
      "clipboardWrite",
      "commands",
      "storage"
    ],
    "action": {
      "default_popup": "popup/main.html"
    },
    "options_ui": {
      "page": "options/main.html",
      "open_in_tab": false
    },
    "background": {
      "service_worker": "background/main.js"
    },
    "content_scripts": [
      {
        "matches": [
          "https://learning.oreilly.com/library/view/*"
        ],
        "css": [
        ],
        "js": [
          "external/chrome-extension-async.js",
          "common/constants.js",
          "common/options.js",
          "common/logger.js",
          "common/service.js",
          "common/utils.js",
          "common/communication.js",
          "content/main.js"
        ]
      }
    ]
  }