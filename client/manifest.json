{
    "manifest_version": 3,
    "name": "Orreilly Downloader",
    "version": "0.0.1",
    "permissions": [
      "activeTab",
      "alarms",
      "tabs",
      "webNavigation",
      "notifications",
      "clipboardRead",
      "clipboardWrite",
      "commands",
      "storage"
    ],
    "options_page": "options/main.html",
    "content_scripts": [
      {
        "matches": [
          "https://learning.oreilly.com/library/view/*"
        ],
        "css": [
        ],
        "js": [
          "common/options.js",
          "common/logger.js",
          "common/service.js",
          "content/main.js"
        ]
      }
    ]
  }