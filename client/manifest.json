    "permissions": [
      "activeTab",
      "alarms",
      "tabs",
      "webNavigation",
      "notifications",
      "clipboardRead",
      "clipboardWrite"
    ],
    "content_scripts": [
      {
        "matches": [
          "https://learning.oreilly.com/library/view/*"
        ],
        "css": [
        ],
        "js": [
          "content/main.js"
        ]
      }
    ]
  }